{
  "extends": "@ffflorian/eslint-config",
  "overrides": [
    {
      "files": "*.ts",
      "rules": {
        "@typescript-eslint/no-unused-vars-experimental": "off",
        "@typescript-eslint/typedef": "off",
        "no-magic-numbers": "off",
        "unused-imports/no-unused-vars": "off"
      }
    }
  ],
  "rules": {
    "valid-jsdoc": "off"
  }
}
